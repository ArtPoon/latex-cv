	Journal = {Methods Mol Biol},
	Journal-Full = {Methods in molecular biology (Clifton, N.J.)},
	Mesh = {Animals; Epistasis, Genetic; Evolution, Molecular; Genome, Viral; Genomics; Humans; Recombination, Genetic; Selection, Genetic},
	Note = {\begin{small}\\\textcolor{violet}{I wrote about a third of this phylogenetic methods tutorial article (including the sections on epistasis and identifying selective factors) and wrote the introduction}\end{small}},
	Pages = {239-72},
	Pmid = {22399462},
	Pst = {ppublish},
