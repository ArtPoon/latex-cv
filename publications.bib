	Mesh = {Bayes Theorem; Drug Resistance, Viral; Evolution, Molecular; Genetic Variation; HIV Infections; HIV Protease; HIV Protease Inhibitors; HIV-1; Humans; Markov Chains; Models, Molecular; Monte Carlo Method; Polymorphism, Genetic},
