	Doi = {10.1128/JVI.00608-09},
	Journal = {J Virol},
	Journal-Full = {Journal of virology},
	Mesh = {Amino Acids; Animals; Bayes Theorem; Computational Biology; Gene Deletion; Genome, Viral; Humans; Influenza A Virus, H5N1 Subtype; Mice; Models, Statistical; Multivariate Analysis; Mutation; Phenotype; Probability; Virulence},
	Month = {Oct},
	Number = {19},
