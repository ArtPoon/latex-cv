	Mesh = {Adaptation, Biological; Bacteriophage phi X 174; Epistasis, Genetic; Evolution, Molecular; Likelihood Functions; Models, Molecular; Mutagenesis, Site-Directed; Mutation; Sequence Analysis, DNA; Viral Plaque Assay},
