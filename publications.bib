	Doi = {10.1097/QAD.0b013e328318f16c},
	Journal = {AIDS},
	Journal-Full = {AIDS (London, England)},
	Mesh = {Drug Resistance, Viral; Female; HIV Infections; HIV Reverse Transcriptase; HIV-1; Humans; Male; Mutation, Missense; RNA, Viral; Retrospective Studies; Reverse Transcriptase Inhibitors; Viral Load},
	Month = {Nov},
