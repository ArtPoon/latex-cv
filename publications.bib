	Mesh = {Adaptation, Physiological; Amino Acid Sequence; Genotype; HIV Infections; HIV-1; HLA Antigens; Humans; Male; Molecular Sequence Data; North America; Phylogeny; Polymorphism, Genetic},
	Month = {Apr},
