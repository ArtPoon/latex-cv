	Doi = {10.1371/journal.pone.0099508},
	Journal = {PLoS One},
	Journal-Full = {PloS one},
	Mesh = {HIV Infections; HIV-1; High-Throughput Nucleotide Sequencing; Humans; Reproducibility of Results; Sequence Analysis, RNA},
	Note = {\begin{small}\\\textcolor{violet}{I assisted in the analysis of the next-generation sequence data and helped revise the manuscript in response to reviewers}\end{small}},
	Number = {6},
