	Mesh = {Amino Acid Sequence; British Columbia; Cohort Studies; Evolution, Molecular; HIV Infections; HIV-1; Humans; Molecular Sequence Data; Phylogeny; Sequence Alignment; Sequence Analysis, RNA; nef Gene Products, Human Immunodeficiency Virus},
	Month = {Apr},
